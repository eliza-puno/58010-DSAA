      "cell_type": "code",
      "source": [
        "class Node:\n",
        "    def __init__(self, data):\n",
        "        self.data = data\n",
        "        self.next = None\n",
        "\n",
        "class Linkedlist:\n",
        "    def __init__(self):\n",
        "        self.head = None\n",
        "\n",
        "llist = Linkedlist()\n",
        "\n",
        "a = 1\n",
        "b = 2\n",
        "c = 3\n",
        "\n",
        "llist.head = Node(a)\n",
        "second = Node(b)\n",
        "third = Node(c)\n",
        "\n",
        "llist.head.next = second\n",
        "second.next = third\n",
        "\n"
      ],
      "metadata": {
        "id": "jRIizwgmW1HM"
      },
      "execution_count": 12,
      "outputs": []
